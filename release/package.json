		"jsonValidation": [
			{
				"fileMatch": "fableconfig.json",
				"url": "./schemas/fableconfig.json"
			}
		],
		"breakpoints": [
			{
				"language": "fsharp"
			}
		],
		"problemMatchers": [
			{
				"name": "fable-webpack",
				"owner": "fsharp",
				"fileLocation": "absolute",
				"background": {
					"activeOnStart": true,
					"beginsPattern": {
						"regexp": "webpack: Compiling"
					},
					"endsPattern": {
						"regexp": "webpack: (Compiled successfully|Failed to compile)"
					}
				},
				"pattern": {
					"regexp": "(.*)\\((\\d+),(\\d+),(\\d+),(\\d+)\\)\\s*:\\s*(warning|error) FABLE\\s*:\\s*(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"endLine": 4,
					"endColumn": 5,
					"severity": 6,
					"message": 7
				}
			}
		],
		"colors": [
			{
				"id": "fsharp.linelens",
				"description": "Foreground color for F# LineLenses",
				"defaults": {
					"dark": "#525252",
					"light": "#C2C2C2",
					"highContrast": "#A2E2A2"
				}
			}
		]
	},
