		"breakpoints": [
			{
				"language": "fsharp"
			}
		],
		"problemMatchers": [
			{
				"name": "fable-webpack",
				"owner": "fsharp",
				"fileLocation": "absolute",
				"background": {
					"activeOnStart": true,
					"beginsPattern": {
						"regexp": "webpack: Compiling"
					},
					"endsPattern": {
						"regexp": "webpack: (Compiled successfully|Failed to compile)"
					}
				},
				"pattern": {
					"regexp": "(.*)\\((\\d+),(\\d+),(\\d+),(\\d+)\\)\\s*:\\s*(warning|error) FABLE\\s*:\\s*(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"endLine": 4,
					"endColumn": 5,
					"severity": 6,
					"message": 7
				}
			}
		],
		"colors": [
			{
				"id": "fsharp.linelens",
				"description": "Foreground color for F# LineLenses",
				"defaults": {
					"dark": "#696969",
					"light": "#919191",
					"highContrast": "#A2E2A2"
				}
			},
			{
				"id": "fsharp.statusBarWarnings",
				"description": "Text colors for warnings shown in the status bar",
				"defaults": {
					"dark": "#FFCC00",
					"light": "#FFCC00",
					"highContrast": "#FFFF00"
				}
			}
		]
